generator client {
  provider = "prisma-client-js"
  //output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  schemas  = ["asa"]
}

model asa_application_contacts {
  application_contact_id Int                   @id @default(autoincrement())
  application_id         Int
  contact_type_id        Int
  contact_name           String?               @db.VarChar(200)
  designation            String?               @db.VarChar(100)
  email_id               String                @db.VarChar(255)
  mobile_no              Decimal?              @db.Decimal(10, 0)
  alt_telephone_no       Decimal?              @db.Decimal(10, 0)
  website_url            String?               @db.VarChar(255)
  helpdesk_no            Decimal?              @db.Decimal(10, 0)
  asa_application_mst    asa_application_mst   @relation(fields: [application_id], references: [application_id], onUpdate: Restrict, map: "fk_ct_app")
  asa_general_value_mst  asa_general_value_mst @relation(fields: [contact_type_id], references: [type_id], onUpdate: Restrict, map: "fk_ct_type")

  @@schema("asa")
}


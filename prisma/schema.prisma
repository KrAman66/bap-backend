  asa_district_mst_asa_application_mst_dist_idToasa_district_mst                asa_district_mst?              @relation("asa_application_mst_dist_idToasa_district_mst", fields: [dist_id], references: [district_id], onDelete: Restrict, onUpdate: Restrict, map: "fk_app_district")
